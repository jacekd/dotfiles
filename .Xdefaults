
Xft*dpi:              96
Xft*antialias:        true
Xft*hinting:          hintslight
Xft*rgba:             rgb
Xft*lcdfilter:        lcdslight
!Xft.hinting:          true
!Xft.autohint:         true

URxvt*font: -misc-fixed-medium-r-semicondensed-*-12-90-100-*-c-60-iso8859-1
