
Xft*dpi:              96
Xft*antialias:        true
Xft*hinting:          hintslight
Xft*rgba:             rgb
Xft*lcdfilter:        lcdslight
!Xft.hinting:          true
!Xft.autohint:         true

URxvt*font: -misc-fixed-medium-r-semicondensed-*-12-90-100-*-c-60-iso8859-1
!URxvt*font: xft:Ubuntu Mono:size=9
URxvt*font: xft:Liberation Mono for Powerline:size=9

URxvt.scrollBar:          false
URxvt.saveLines:          20000
URxvt*lineSpace:          5
URxvt.letterSpace:        -2
URxvt*cursorColor:        #cc0000
URxvt*internalBorder:     5

! Extensions
URxvt.perl-lib:           /usr/lib/urxvt/perl
URxvt.perl-ext-common:    default,matcher,clipboard,copypaste
URxvt.matcher.launcher:   x-www-browser
URxvt.matcher.button:     1
URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]+\\.[\\w./?&@#-=]*[\\w/-]
!URxvt.keysym.Mod4-c:      perl:clipboard:copy
!URxvt.keysym.Mod4-v:      perl:clipboard:paste
!URxvt.keysym.S-Insert:    perl:clipboard:paste


*background: #f1f1f1
*foreground: #333333
!black
*color0:  #222222
*color8:  #444444
!red
*color1:  #CC0000
*color9:  #FF4444
!green
*color2:  #669900
*color10: #99CC00
!yellow
*color3:  #FF8800
*color11: #FFBB33
!blue
*color4:  #0099CC
*color12: #33B5E5
!magenta
*color5:  #822873
*color13: #D255BE
!cyan
*color6:  #038e82
*color14: #05d2c1
!white
*color7:  #ffffff
*color15: #efefef
